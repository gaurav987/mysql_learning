
create table orders_data
(
  cust_id int,
  order_id int,
  country varchar(100),
  state varchar(50)
);

insert into orders_data values(1,100,'USA','Seattle');
insert into orders_data values(2,101,'INDIA','UP');
insert into orders_data values(2,103,'INDIA','Bihar');
